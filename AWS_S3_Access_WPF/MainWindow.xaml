        <ListBox x:Name="listbox_bucketList" HorizontalAlignment="Left" Height="132" Margin="137,109,0,0" VerticalAlignment="Top" Width="582"/>
        <ListBox x:Name="listbox_bucketObjList" HorizontalAlignment="Left" Height="132" Margin="137,282,0,0" VerticalAlignment="Top" Width="582"/>
