        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWS_S3_Access_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="549.769" Width="800" Initialized="Window_Initialized">
